<UserControl x:Class="AcademicSavingService.View.WithdrawsManager"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AcademicSavingService.View"
			 xmlns:ctrl="clr-namespace:AcademicSavingService.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
		<ctrl:AssDataGrid EnableFullTextSearch="True" x:Name="asd" ItemsSource="{Binding Tests}">
			<ctrl:AssDataGrid.Columns>
				<DataGridTextColumn Header="ID" Binding="{Binding shit}"/>
				<DataGridTextColumn Header="ID2" Width="10" Binding="{Binding shit2}"/>
				<DataGridTextColumn Header="ID3" Width="100" Binding="{Binding shit3}"/>
				<DataGridTextColumn Header="ID4" Binding="{Binding shit4}"/>
				<DataGridTextColumn Header="ID5"/>
			</ctrl:AssDataGrid.Columns>
		</ctrl:AssDataGrid>
	</Grid>
</UserControl>
